# Bot Framework Configuration
APP_ID=your-bot-app-id-here
APP_PASSWORD=your-bot-app-password-here
APP_TYPE=SingleTenant
APP_TENANTID=your-tenant-id-here


# Databricks Configuration
DATABRICKS_SPACE_ID=your-databricks-space-id
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com/
DATABRICKS_TOKEN=your-databricks-token-here

# Sample Questions Configuration
# Customize these questions for your Genie space - use semicolons (;) to separate multiple questions
# These will be shown to users when they first log in
SAMPLE_QUESTIONS=What data is available in this Genie space?;Can you explain the datasets?;What questions should I ask?

# Admin Contact Email
# This email is displayed to users in the info command for support inquiries
ADMIN_CONTACT_EMAIL=admin@company.com

# Application Configuration
PORT=3978

# Feedback Configuration
ENABLE_FEEDBACK_CARDS=True
ENABLE_GENIE_FEEDBACK_API=True
